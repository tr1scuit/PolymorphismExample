
public class Pizza extends Food {
	
// The child class of Food

	private String name;
	
	public Pizza(String name){
		this.name = name;
		// This constructor is for a specific burger that
		// has a special name (For Driver_Food_Arrays)
	}
	
	public Pizza(){
		// This constructor is for a generic burger
		// that doesn't have a name (For Driver_Food)
	}

	public void eat(){
		System.out.println("This PIZZA is delicious!");
	}
	
	public String getName(){
		return name;
	}
	// To be used in Driver_Food_Arrays

}
